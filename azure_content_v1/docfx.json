{
  "build": {
    "content": [
      {
        "files": [
          "articles/active-directory/**/*.md"
        ],
        "exclude": [
          "**/obj/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "**/*.md",
          "**/*.png",
          "**/*.jpg",
          "**/*.gif",
          "_themes/**"
        ],
        "exclude": [
          "**/obj/**",
          "articles/active-directory/**/*.md",
          "_themes/DocPacker/**"
        ]
      }
    ],
    "template": [
      "docs.html"
    ],
    "dest": "azure_content_v1"
  }
}